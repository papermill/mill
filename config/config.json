{
  "// ___ MILL CLI CONFIG ": "___",
  "banner": [
    "              _   _  ",
    "          o  | | | | ",
    "_  _  _      | | | | ",
    " |/ |/ |  |  |/  |/  ",
    " |  |  |_/|_/|__/|__/",
    ""
  ],
  
  "// # ": " SOME CONSTANTS ",
  "DEFAULT_CONFIGFILE": "papermill.json",
  "CONFIGFILES": [
    "_papermill/config.json",
    "_papermill/papermill.json",
    "papermill/config.json",
    "papermill/papermill.json",
    ".papermill/config.json",
    ".papermill/papermill.json",
    ".papermill",
    ".papermill.json",
    "papermill.json"
  ],
  "MD_EXTENSIONS": [
    "markdown",
    "mkdn",
    "mdown",
    "md"
  ],
  "AUTO_EXCLUDED_DIRS": [
    "node_modules",
    ".tmp",
    ".papermill"
  ],
   
  "stationery": [
    {
      "name": "default",
      "url": "https://github.com/papermill/stationery"
    }
  ],
  
  "//": " Debug Stuff ",
  "DEBUG": {
    "//": "set this to true for lots of info",
    "on": false,
    "workingdir": null
  },
  
  "// # ": " DEFAULT PAPERMILL CONFIG ",
  "papermill": {
    "targets": [
      "print", "web"
    ],
    "output_dir": "_output",
    "known_props": [
      "bibliography"
    ],
    "internal_config_mapping": [
      ["format", "read"],
      ["class", "documentclass"]
    ],
    "pandoc": {
      "read": "markdown",
      "standalone": true,
      "selfContained": true,
      "smart": true,
      "toc": true,
      "normalize": true,
      "numberSections": true
    },
    "pandoc_targets": {
      "web": {
        "write": "html5",
        "sectionDivs": true,
        "variable": {
          "webfont": true,
          "scripts": true
        }
      },
      "print": {
        "class": "report",
        "latexEngine": "xelatex",
        "variable": {
          "links-as-notes": true,
          "verbatim-in-note": true
        }
      }
    }
  }
}